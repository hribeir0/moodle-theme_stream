{{!
    @template theme_stream/partials/bigfooter

    Bigger footer with extra info and social media.

    Example context (json):
    {
        "output": {
            "page_doc_link": "Help and documentation",
            "supportemail": "<a href=\"#\">Contact site support</a>",
            "has_popover_links": true,
            "services_support": "Services and support",
            "login_info": "You are logged in as cute kitten",
            "moodle_release": "90210",
            "leftcolumn": "<ul><li>List item</li></ul>",
            "centercolumn": "<p>lorem ipsum</p>",
            "facebookurl": "https://url.com",
            "instagramurl": "https://url.com",
            "pinteresturl": "https://url.com",
            "youtubeurl": "https://url.com",
            "linkedinurl": "https://url.com",
            "twitterurl": "https://url.com"
        }
    }
}}
    <!-- Grid container -->
  <div class="container-fluid stream-footer footer-dark text-light">
    <!--Grid row-->
    <div class="row m-0">
      <!--Grid column-->
      <div class="col-lg-4 col-md-4">
        {{{leftcolumn}}}
      </div>
      <!--Grid column-->

      <!--Grid column-->
      <div class="col-lg-4 col-md-4">
        {{{centercolumn}}}
      </div>
      <!--Grid column-->

      <!--Grid column-->
      <div class="col-lg-4 col-md-4">
        {{# output.has_popover_links }}
            <div class="footer-section p-3 support-column">
                {{# output.page_doc_link }}
                    <div>{{{ output.page_doc_link }}}</div>
                {{/ output.page_doc_link }}

                {{# output.services_support_link }}
                    <div>{{{ output.services_support_link }}}</div>
                {{/ output.services_support_link }}

                {{# output.supportemail }}
                    <div>{{{ output.supportemail }}}</div>
                {{/ output.supportemail }}
                <div class="tool_usertours-resettourcontainer">
                </div>
            </div>
        {{/ output.has_popover_links }}


      </div>
      <!--Grid column-->
    </div>
    <!--Grid row-->
    <div class="row container-fluid m-0">
      <div class="col-lg-12 text-center socialmedia">
        {{#facebookurl}}
        <a href="{{facebookurl}}" aria-label="Facebook"><i class="fa-brands fa-facebook-f"></i></a>
        {{/facebookurl}}
        {{#instagramurl}}
        <a href="{{instagramurl}}" aria-label="Instagram"><i class="fa-brands fa-instagram"></i></a>
        {{/instagramurl}}
        {{#twitterurl}}
        <a href="{{twitterurl}}" aria-label="Twitter"><i class="fa-brands fa-x-twitter"></i></a>
        {{/twitterurl}}
        {{#youtubeurl}}
        <a href="{{youtubeurl}}" aria-label="Youtube"><i class="fa-brands fa-youtube"></i></a>
        {{/youtubeurl}}
        {{#linkedinurl}}
        <a href="{{linkedinurl}}" aria-label="Linkedin"><i class="fa-brands fa-linkedin-in"></i></a>
        {{/linkedinurl}}
        {{#pinteresturl}}
        <a href="{{pinteresturl}}" aria-label="Pinterest"><i class="fa-brands fa-pinterest-p"></i></a>
        {{/pinteresturl}}
      </div>
    </div>
  </div>
  <!-- Grid container -->